;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Tictactoe;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Components;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;    (role white)    (role black)    (<= (base (cell ?m ?n ?b white)) (index ?m) (index ?n) (board ?b))    (<= (base (cell ?m ?n ?b black)) (index ?m) (index ?n) (board ?b))    (<= (base (cell ?m ?n ?b blank)) (index ?m) (index ?n) (board ?b))    (base (control white))    (base (control black))    (<= (input ?r (mark ?m ?n ?b)) (role ?r) (index ?m) (index ?n) (board ?b))    (<= (input ?r noop) (role ?r))    (index 1)    (index 2)    (index 3)    (board 1)    (board 2)  ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; init;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;    (init (cell 1 1 1 b))    (init (cell 1 2 1 b))    (init (cell 1 3 1 b))    (init (cell 2 1 1 b))    (init (cell 2 2 1 b))    (init (cell 2 3 1 b))    (init (cell 3 1 1 b))    (init (cell 3 2 1 b))    (init (cell 3 3 1 b))    (init (cell 1 1 2 b))    (init (cell 1 2 2 b))    (init (cell 1 3 2 b))    (init (cell 2 1 2 b))    (init (cell 2 2 2 b))    (init (cell 2 3 2 b))    (init (cell 3 1 2 b))    (init (cell 3 2 2 b))    (init (cell 3 3 2 b))    (init (control white))    ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; legal;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;            (<= (legal ?r (mark ?m ?n ?b))        (true (cell ?m ?n ?b blank))        (true (control ?r)))        (<= (legal white noop)        (true (control black)))        (<= (legal black noop)        (true (control white)))    ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; next;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;    (<= (next (cell ?m ?n ?b x))        (does white (mark ?m ?n))        (true (cell ?m ?n ?b b)))        (<= (next (cell ?m ?n ?b o))        (does black (mark ?m ?n))        (true (cell ?m ?n ?b b)))        (<= (next (cell ?m ?n ?b ?w))        (true (cell ?m ?n ?b ?w))        (distinct ?w b))        (<= (next (cell ?m ?n ?b b))        (does ?w (mark ?j ?k))        (true (cell ?m ?n ?b b))        (distinct ?m ?j))        (<= (next (cell ?m ?n ?b b))        (does ?w (mark ?j ?k))        (true (cell ?m ?n ?b b))        (distinct ?n ?k))        (<= (next (control white))        (true (control black)))        (<= (next (control black))        (true (control white)))            (<= (row ?m ?x)        (true (cell ?m 1 ?x))        (true (cell ?m 2 ?x))        (true (cell ?m 3 ?x)))        (<= (column ?n ?x)        (true (cell 1 ?n ?x))        (true (cell 2 ?n ?x))        (true (cell 3 ?n ?x)))        (<= (diagonal ?x)        (true (cell 1 1 ?x))        (true (cell 2 2 ?x))        (true (cell 3 3 ?x)))        (<= (diagonal ?x)        (true (cell 1 3 ?x))        (true (cell 2 2 ?x))        (true (cell 3 1 ?x)))            (<= (line ?x) (row ?m ?x))    (<= (line ?x) (column ?m ?x))    (<= (line ?x) (diagonal ?x))            (<= open (true (cell ?m ?n ?b b)))    ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; goal;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;        (<= (goal white 100)        (line x)        (not (line o)))        (<= (goal white 50)        (not (line x))        (not (line o)))        (<= (goal white 0)        (not (line x))        (line o))    (<= (goal black 100)        (not (line x))        (line o))          (<= (goal black 50)        (not (line x))        (not (line o)))      (<= (goal black 0)        (line x)        (not (line o)))    ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; terminal;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;        (<= terminal        (line x))        (<= terminal        (line o))        (<= terminal        (not open));;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;